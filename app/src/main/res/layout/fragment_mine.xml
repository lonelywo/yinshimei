<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F1F1F1"
    >
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:fitsSystemWindows="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    <ScrollView
        android:id="@+id/scroll_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="none"
        >

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:descendantFocusability="blocksDescendants"

            >

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="150dp"
                android:src="@drawable/shape_mine_bg1"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/con_toubu"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:text="我的"
                    android:textColor="#ffffff"
                    android:textSize="20sp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <ImageView
                android:id="@+id/img_shape_bai"
                android:layout_width="match_parent"
                android:layout_height="120dp"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="16dp"
                android:scaleType="fitXY"
                android:src="@drawable/shape_mine_lv_bg"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/con_toubu" />

            <ImageView
                android:id="@+id/img_headwear"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="8dp"
                android:layout_marginTop="10dp"
                android:src="@drawable/headwear"
                android:visibility="gone"
                app:layout_constraintStart_toStartOf="@+id/img_tuxiang"
                app:layout_constraintTop_toTopOf="@+id/img_shape_bai" />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/img_tuxiang"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_marginLeft="20dp"
                android:src="@drawable/tuxiang"
                app:civ_border_color="@color/home_huang"
                app:civ_border_width="2dp"
                app:layout_constraintBottom_toBottomOf="@+id/img_shape_bai"
                app:layout_constraintLeft_toLeftOf="@+id/img_shape_bai"
                app:layout_constraintTop_toTopOf="@+id/img_shape_bai" />

            <TextView
                android:id="@+id/img_yqhy"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginRight="20dp"
                android:drawableLeft="@drawable/yqhy"
                android:drawablePadding="6dp"
                android:paddingLeft="3dp"
                android:text="邀请好友"
                android:visibility="visible"
                app:layout_constraintRight_toRightOf="@+id/img_shape_bai"
                app:layout_constraintTop_toBottomOf="@+id/text_name" />

            <TextView
                android:id="@+id/text_name"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="8dp"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="请登录"
                android:textColor="#000000"
                android:textSize="16sp"
                app:layout_constraintLeft_toRightOf="@+id/img_tuxiang"
                app:layout_constraintRight_toLeftOf=" @+id/img_yqhy"
                app:layout_constraintTop_toTopOf="@+id/img_tuxiang" />
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="8dp"
                android:background="@drawable/shape_nicheng"
                android:gravity="center"
                app:layout_constraintBottom_toBottomOf="@+id/img_tuxiang"
                app:layout_constraintLeft_toLeftOf="@+id/text_name"
                android:paddingLeft="@dimen/dp_10"
                android:paddingRight="@dimen/dp_10"
                >

                <ImageView
                    android:id="@+id/img_jingxiao"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginTop="1dp"
                    android:src="@drawable/diamonds" />

                <TextView
                    android:id="@+id/text_huiyuan"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="3dp"
                    android:text="用户"
                    android:textColor="#E1AD73"
                    android:textSize="12sp" />
            </LinearLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/con_dingdan"
                android:layout_width="match_parent"
                android:layout_height="150dp"
                android:layout_marginTop="10dp"
                app:layout_constraintTop_toBottomOf="@+id/img_shape_bai">

                <ImageView
                    android:id="@+id/img_shape_bai2"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginRight="16dp"
                    android:src="@drawable/shape_mine_bg3"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/text_dingdan"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:padding="10dp"
                    android:text="我的订单"
                    android:textColor="#000000"
                    android:textSize="15sp"
                    app:layout_constraintLeft_toLeftOf="@+id/img_shape_bai2"
                    app:layout_constraintTop_toTopOf="@+id/img_shape_bai2" />

                <TextView
                    android:id="@+id/text_quanbudingdan"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="10dp"
                    android:drawableRight="@drawable/youjiantou"
                    android:drawablePadding="6dp"
                    android:text="我的消费"
                    android:textColor="#7D7D7D"
                    android:textSize="12sp"
                    app:layout_constraintBottom_toBottomOf="@+id/text_dingdan"
                    app:layout_constraintRight_toRightOf="@+id/img_shape_bai2"
                    app:layout_constraintTop_toTopOf="@+id/text_dingdan"

                    />

                <View
                    android:id="@+id/view"
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_marginTop="5dp"
                    android:background="#f8f8f8"
                    app:layout_constraintLeft_toLeftOf="@+id/img_shape_bai2"
                    app:layout_constraintRight_toRightOf="@+id/img_shape_bai2"
                    app:layout_constraintTop_toBottomOf="@+id/text_dingdan" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:padding="10dp"
                    app:layout_constraintBottom_toBottomOf="@+id/img_shape_bai2"
                    app:layout_constraintEnd_toEndOf="@+id/img_shape_bai2"
                    app:layout_constraintStart_toStartOf="@+id/img_shape_bai2"
                    app:layout_constraintTop_toTopOf="@+id/view">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/daifukuan_ll"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_weight="1"
                        tools:layout_editor_absoluteX="20dp"
                        tools:layout_editor_absoluteY="19dp">

                        <TextView
                            android:id="@+id/text_daifukuan"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:drawableTop="@drawable/daifukuan"
                            android:drawablePadding="6dp"
                            android:gravity="center"
                            android:text="待付款"
                            android:textColor="#646262"
                            app:layout_constraintBottom_toBottomOf="parent"


                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <TextView
                            android:id="@+id/dot1_tv"
                            android:layout_width="16dp"
                            android:layout_height="16dp"
                            android:background="@drawable/shape_mine_order_status_num"
                            android:gravity="center"
                            android:textColor="#e1ad73"
                            android:textSize="11sp"
                            android:visibility="gone"
                            app:layout_constraintEnd_toEndOf="@+id/text_daifukuan"
                            app:layout_constraintTop_toTopOf="parent"
                            tools:text="3" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/daifahuo_ll"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_weight="1">

                        <TextView
                            android:id="@+id/text_daifahuo"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:drawableTop="@drawable/daifahuo"
                            android:drawablePadding="6dp"
                            android:gravity="center"
                            android:text="待发货"
                            android:textColor="#646262"
                            app:layout_constraintBottom_toBottomOf="parent"


                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <TextView
                            android:id="@+id/dot2_tv"
                            android:layout_width="16dp"
                            android:layout_height="16dp"
                            android:background="@drawable/shape_mine_order_status_num"
                            android:gravity="center"
                            android:textColor="@color/orange"
                            android:textSize="11dp"
                            android:visibility="gone"
                            app:layout_constraintEnd_toEndOf="@+id/text_daifahuo"
                            app:layout_constraintTop_toTopOf="parent"
                            tools:text="3" />
                    </androidx.constraintlayout.widget.ConstraintLayout>


                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/daishouhuo_ll"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_weight="1">

                        <TextView
                            android:id="@+id/text_daishouhuo"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:drawableTop="@drawable/daishouhuo"
                            android:drawablePadding="6dp"
                            android:gravity="center"
                            android:text="待收货"
                            android:textColor="#646262"
                            app:layout_constraintBottom_toBottomOf="parent"


                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <TextView
                            android:id="@+id/dot3_tv"
                            android:layout_width="16dp"
                            android:layout_height="16dp"
                            android:background="@drawable/shape_mine_order_status_num"
                            android:gravity="center"
                            android:textColor="@color/orange"
                            android:textSize="11dp"
                            android:visibility="gone"
                            app:layout_constraintEnd_toEndOf="@+id/text_daishouhuo"
                            app:layout_constraintTop_toTopOf="parent"
                            tools:text="3" />
                    </androidx.constraintlayout.widget.ConstraintLayout>


                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/yiwancheng_ll"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_weight="1">

                        <TextView
                            android:id="@+id/text_yiwancheng"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:drawableTop="@drawable/yiwancheng"
                            android:drawablePadding="6dp"
                            android:gravity="center"
                            android:text="已完成"
                            android:textColor="#646262"
                            app:layout_constraintBottom_toBottomOf="parent"


                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <TextView
                            android:id="@+id/dot4_tv"
                            android:layout_width="16dp"
                            android:layout_height="16dp"
                            android:background="@drawable/shape_mine_order_status_num"
                            android:gravity="center"
                            android:textColor="@color/orange"
                            android:textSize="11dp"
                            android:visibility="gone"
                            app:layout_constraintEnd_toEndOf="@+id/text_yiwancheng"
                            app:layout_constraintTop_toTopOf="parent"
                            tools:text="3" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                </LinearLayout>


            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/con_yingchang"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:visibility="gone"
                app:layout_constraintTop_toBottomOf="@+id/con_dingdan">

                <ImageView
                    android:id="@+id/img_shape_bai3"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginRight="16dp"
                    android:src="@drawable/shape_mine_bg3"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/text_fuwu"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:padding="10dp"
                    android:text="我的服务"
                    android:textColor="#000000"
                    android:textSize="15sp"
                    app:layout_constraintLeft_toLeftOf="@+id/img_shape_bai3"
                    app:layout_constraintTop_toTopOf="@+id/img_shape_bai3" />

                <View
                    android:id="@+id/view1"
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_marginTop="5dp"
                    android:background="#f8f8f8"
                    app:layout_constraintLeft_toLeftOf="@+id/img_shape_bai3"
                    app:layout_constraintRight_toRightOf="@+id/img_shape_bai3"
                    app:layout_constraintTop_toBottomOf="@+id/text_fuwu" />

                <LinearLayout
                    android:id="@+id/ll_fuwu"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:padding="10dp"
                    app:layout_constraintEnd_toEndOf="@+id/img_shape_bai3"
                    app:layout_constraintStart_toStartOf="@+id/img_shape_bai3"
                    app:layout_constraintTop_toTopOf="@+id/view1">

                    <TextView
                        android:id="@+id/text_tuiguangyongjing"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:layout_weight="1"
                        android:drawableTop="@drawable/tgyongjing"
                        android:drawablePadding="6dp"
                        android:gravity="center_horizontal"
                        android:text="我的收益"
                        android:textColor="#646262"

                        />

                    <TextView
                        android:id="@+id/text_wodetuandui"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:layout_weight="1"
                        android:drawableTop="@drawable/wdtuandui"
                        android:drawablePadding="6dp"
                        android:gravity="center_horizontal"
                        android:text="我的美粉"
                        android:textColor="#646262" />

                    <TextView
                        android:id="@+id/text_yejiyuefan"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:layout_weight="1"
                        android:drawableTop="@drawable/yejiyuefan"
                        android:drawablePadding="6dp"
                        android:gravity="center_horizontal"
                        android:text="我的福利"
                        android:textColor="#646262" />

                    <TextView
                        android:id="@+id/text_pk"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:layout_weight="1"
                        android:drawableTop="@drawable/pk"
                        android:drawablePadding="6dp"
                        android:gravity="center_horizontal"
                        android:text="我的排行"
                        android:textColor="#646262" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_fuwu1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:padding="10dp"
                    app:layout_constraintEnd_toEndOf="@+id/img_shape_bai3"
                    app:layout_constraintStart_toStartOf="@+id/img_shape_bai3"
                    app:layout_constraintTop_toBottomOf="@+id/ll_fuwu">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/goods_ll"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_weight="1"
                        android:visibility="gone">

                        <TextView
                            android:id="@+id/text_shareliwu"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:drawableTop="@drawable/receivegoods"
                            android:drawablePadding="6dp"
                            android:text="礼品中心"
                            android:textColor="#646262"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <TextView
                            android:id="@+id/dot1_goods"
                            android:layout_width="6dp"
                            android:layout_height="6dp"
                            android:background="@drawable/shape_mine_hx_status_num"
                            android:gravity="center"
                            android:text=""
                            android:textColor="@color/colorWhite"
                            android:textSize="12sp"
                            android:visibility="gone"
                            app:layout_constraintEnd_toEndOf="@+id/text_shareliwu"
                            app:layout_constraintTop_toTopOf="@+id/text_shareliwu" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <TextView
                        android:id="@+id/text_wodekajuan1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:layout_weight="1"
                        android:drawablePadding="6dp"
                        android:gravity="center_horizontal"
                        android:text=""
                        android:textColor="#646262" />

                    <TextView
                        android:id="@+id/text_wodetuandui1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:layout_weight="1"
                        android:drawablePadding="6dp"
                        android:gravity="center_horizontal"
                        android:text=""
                        android:textColor="#646262" />

                    <TextView
                        android:id="@+id/text_pk1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:layout_weight="1"
                        android:drawablePadding="6dp"
                        android:gravity="center_horizontal"
                        android:text=""
                        android:textColor="#646262" />
                </LinearLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/con_gongju"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                app:layout_constraintTop_toBottomOf="@+id/con_yingchang">

                <ImageView
                    android:id="@+id/img_shape_bai4"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginRight="16dp"
                    android:src="@drawable/shape_mine_bg3"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/text_gongju"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:padding="10dp"
                    android:text="我的工具"
                    android:textColor="#000000"
                    android:textSize="15sp"
                    app:layout_constraintLeft_toLeftOf="@+id/img_shape_bai4"
                    app:layout_constraintTop_toTopOf="@+id/img_shape_bai4" />

                <View
                    android:id="@+id/view2"
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_marginTop="5dp"
                    android:background="#f8f8f8"
                    app:layout_constraintLeft_toLeftOf="@+id/img_shape_bai4"
                    app:layout_constraintRight_toRightOf="@+id/img_shape_bai4"
                    app:layout_constraintTop_toBottomOf="@+id/text_gongju" />

                <LinearLayout
                    android:id="@+id/ll_fuwu2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:padding="@dimen/dp_10"
                    app:layout_constraintEnd_toEndOf="@+id/img_shape_bai4"
                    app:layout_constraintStart_toStartOf="@+id/img_shape_bai4"
                    app:layout_constraintTop_toBottomOf="@+id/view2">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/wodekefu_ll"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_weight="1">

                        <TextView
                            android:id="@+id/text_wodekefu"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:drawableTop="@drawable/wodekefu"
                            android:drawablePadding="6dp"
                            android:text="我的客服"
                            android:textColor="#646262"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <TextView
                            android:id="@+id/dot1_hx"
                            android:layout_width="6dp"
                            android:layout_height="6dp"
                            android:background="@drawable/shape_mine_hx_status_num"
                            android:gravity="center"
                            android:text=""
                            android:textSize="12sp"
                            android:visibility="gone"
                            app:layout_constraintEnd_toEndOf="@+id/text_wodekefu"
                            app:layout_constraintTop_toTopOf="@+id/text_wodekefu" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/shouhuodizi_ll"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_weight="1">

                        <TextView
                            android:id="@+id/text_shouhuodizi"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:drawableTop="@drawable/dizi"
                            android:drawablePadding="6dp"
                            android:text="收货地址"
                            android:textColor="#646262"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
