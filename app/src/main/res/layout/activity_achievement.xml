<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/con_toubu"
        android:layout_width="match_parent"
        android:layout_height="65dp"
        app:layout_constraintTop_toTopOf="parent"
        >
        <ImageView
            android:id="@+id/img_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icon_back"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="20dp"
            />
        <TextView
            android:id="@+id/text_biaoti"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="业绩月返"
            android:textColor="@color/hei"
            android:textSize="20sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <com.tencent.smtt.sdk.WebView
        android:id="@+id/web_guize"
        android:layout_width="match_parent"
        android:layout_height="350dp"
        app:layout_constraintTop_toBottomOf="@+id/con_toubu"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_margin="20dp"
        />
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/con_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/web_guize"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
           app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            android:text="上月回购"
            android:textColor="@color/hei"
            android:textSize="20sp"
            />
        <TextView
            android:id="@+id/text_weidabiao"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
           app:layout_constraintEnd_toEndOf="parent"
           android:layout_marginRight="20dp"
            android:layout_marginTop="20dp"
            android:text="未达标"
            android:textColor="@color/orange"
            android:textSize="20sp"
            />
       <View
           android:id="@+id/line1"
           android:layout_width="match_parent"
           android:layout_height="0.5dp"
           android:background="@drawable/shape_login_edit_bottom_line"
           app:layout_constraintTop_toBottomOf="@+id/text_weidabiao"
           android:layout_marginTop="20dp"
           />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/con_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/con_1"
        >
        <TextView
            android:id="@+id/text_kelingqu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            android:text="上月可领礼品"
            android:textColor="@color/hei"
            android:textSize="20sp"
            />
        <TextView
            android:id="@+id/text_weidabiao1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@+id/text_kelingqu"
           app:layout_constraintBottom_toBottomOf="@+id/text_kelingqu"
           app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginRight="20dp"
            android:text="-"
            android:textColor="@color/orange"
            android:textSize="20sp"

            />
        <View
            android:id="@+id/line2"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@drawable/shape_login_edit_bottom_line"
            app:layout_constraintTop_toBottomOf="@+id/text_kelingqu"
            android:layout_marginTop="20dp"
            />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/con_3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/con_2"
        android:paddingBottom="20dp"
        >
        <TextView
            android:id="@+id/text_fafang"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            android:text="礼品发放状态"
            android:textColor="@color/hei"
            android:textSize="20sp"
            />
        <TextView
            android:id="@+id/text_weidabiao2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@+id/text_fafang"
            app:layout_constraintBottom_toBottomOf="@+id/text_fafang"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginRight="20dp"
            android:text="-"
            android:textColor="@color/orange"
            android:textSize="20sp"

            />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>